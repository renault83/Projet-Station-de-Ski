    private void créeActionPerformed(java.awt.event.ActionEvent evt) {
        String url = "jdbc:mysql://localhost:3306/ski";
        String user = "root";
        String password = "admin";

        String nom = jTextField1.getText();
        String prenom = jTextField2.getText();
        String email = jTextField3.getText();
        String ville = jTextField4.getText();
        String code_postal = jTextField6.getText();

         int jour = Integer.parseInt(jTextField10.getText());
         int mois = Integer.parseInt(jTextField11.getText());
         int annee = Integer.parseInt(jTextField12.getText());

         java.time.LocalDate dateNaissance = java.time.LocalDate.of(annee, mois, jour);
         String date_naissance = dateNaissance.toString();

         String sql = "INSERT INTO clients (nom, prenom, email, ville, code_postal, date_naissance) "
               + "VALUES (?, ?, ?, ?, ?, ?)";

          try {

        Connection conn = DriverManager.getConnection(url, user, password);
        PreparedStatement pst = conn.prepareStatement(sql);

        pst.setString(1, nom);
        pst.setString(2, prenom);
        pst.setString(3, email);
        pst.setString(4, ville);
        pst.setString(5, code_postal);
        pst.setString(6, date_naissance);

        pst.executeUpdate();

        JOptionPane.showMessageDialog(this, "Client ajouté avec succès ✅");

            pst.close();
    conn.close();

} catch (NumberFormatException e) {
    JOptionPane.showMessageDialog(this, "Veuillez entrer des nombres valides pour la date !");
    return;
} catch (java.time.DateTimeException e) {
    JOptionPane.showMessageDialog(this, "Date de naissance invalide !");
    return;
} catch (Exception e) {
    JOptionPane.showMessageDialog(this, "Erreur : " + e.getMessage());
    return;
}


